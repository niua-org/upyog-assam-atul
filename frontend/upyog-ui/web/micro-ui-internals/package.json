{
  "name": "egovernments",
  "version": "1.5.4",
  "main": "index.js",
  "workspaces":  [
    "packages/libraries",
    "packages/css",
    "example",
    "packages/react-components",
    "packages/modules/core",
    "packages/modules/common",
    "packages/modules/hrms",
    "packages/modules/obpsv2",
    "packages/modules/noc"
  ],
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "run-s build start:dev",
    "sprint": "SKIP_PREFLIGHT_CHECK=true run-s start:script",
    "start:dev": "run-p dev:**",
    "start:script": "./scripts/create.sh",
    "devD:css": "cd packages/css && yarn start",
    "publish:css": "cd packages/css && yarn publish --access public",
    "dev:libraries": "cd packages/libraries && yarn start",
    "dev:components": "cd packages/react-components && yarn start",
    "dev:common": "cd packages/modules/common && yarn start",
    "dev:core": "cd packages/modules/core && yarn start",
    "dev:receipts": "cd packages/modules/receipts && yarn start",
    "dev:hrms": "cd packages/modules/hrms && yarn start",
    "dev:obpsv2": "cd packages/modules/obpsv2 && yarn start",
    "dev:engagement": "cd packages/modules/engagement && yarn start",
    "dev:bills": "cd packages/modules/bills && yarn start",
    "dev:noc": "cd packages/modules/noc && yarn start",
    "dev:example": "cd example && yarn start",
    "build": "run-p build:**",
    "build:libraries": "cd packages/libraries && yarn build",
    "build:components": "cd packages/react-components && yarn build",
    "build:common": "cd packages/modules/common && yarn build",
    "build:core": "cd packages/modules/core && yarn build",
    "buildD:receipts": "cd packages/modules/receipts && yarn build",
    "build:hrms": "cd packages/modules/hrms && yarn build",
    "build:obpsv2": "cd packages/modules/obpsv2 && yarn build",
    "build:noc": "cd packages/modules/noc && yarn build",
    "buildD:engagement": "cd packages/modules/engagement && yarn build",
    "buildD:bills": "cd packages/modules/bills && yarn build",
    "deploy:jenkins": "./scripts/jenkins.sh"
  },
  "devDependencies": {
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2"
  },
  "husky": {},
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "microbundle-crl": "^0.13.11"
  },
  "resolutions": {
    "**/babel-loader": "8.2.2",
    "**/@babel/core": "7.14.0",
    "**/@babel/preset-env": "7.14.0",
    "**/@babel/plugin-transform-modules-commonjs": "7.14.0",
    "**/styled-components": "5.0.0",
    "**/q": "1.5.1",
    "**/@jridgewell/gen-mapping": "0.3.5",
    "**/polished":"4.2.2",
    "**/cssesc": "3.0.0",
    "glob":"7.2.3",
    "minimatch":"3.1.2"
  }
}
